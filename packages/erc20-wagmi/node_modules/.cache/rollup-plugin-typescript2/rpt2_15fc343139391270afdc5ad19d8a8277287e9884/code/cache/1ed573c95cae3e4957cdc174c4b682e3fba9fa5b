{"code":"import { utils } from 'ethers';\r\nimport * as React from \"react\";\r\nimport { useForm } from \"react-hook-form\";\r\nimport classNames from \"classnames\";\r\nimport useERC20ContractWrite from './useERC20ContractWrite';\r\nexport const ERC20TransferFrom = ({ className, onUpdate, token, symbol }) => {\r\n    const styleForm = classNames(className, \"ERC20TransferFrom\");\r\n    const { register, handleSubmit, watch, formState: {}, } = useForm({\r\n        defaultValues: {\r\n            from: '',\r\n            to: '',\r\n            amount: '',\r\n        },\r\n    });\r\n    const watchAllFields = watch();\r\n    const { write } = useERC20ContractWrite(token, \"transferFrom\", [watchAllFields?.from, watchAllFields?.to, utils.parseEther(watchAllFields.amount || '0')]);\r\n    const onSubmit = (_data) => {\r\n        write();\r\n        if (onUpdate)\r\n            onUpdate(_data);\r\n    };\r\n    const InputWithLabel = ({ className, name, label, register, required, placeholder, type, }) => (React.createElement(\"div\", { className: className },\r\n        React.createElement(\"div\", { className: \"flex items-center justify-center\" },\r\n            React.createElement(\"input\", Object.assign({ className: \"border-0 px-3 py-3 placeholder-neutral-400 text-gray-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\", type: type, placeholder: placeholder }, register(name, { required }))),\r\n            React.createElement(\"label\", { className: \"px-3 py-2 bg-emerald-500 text-white text-center rounded-smd ml-2\" },\r\n                React.createElement(\"span\", { className: \"uppercase text-xs font-bold\" }, label)))));\r\n    return (React.createElement(\"div\", { className: styleForm },\r\n        React.createElement(\"form\", { onSubmit: handleSubmit(onSubmit) },\r\n            React.createElement(\"div\", { className: \"mt-4\" },\r\n                React.createElement(InputWithLabel, { name: \"from\", label: \"From\", placeholder: \"0x000...000\", register: register }),\r\n                React.createElement(InputWithLabel, { name: \"to\", label: \"To\", placeholder: \"0x000...000\", register: register }),\r\n                React.createElement(InputWithLabel, { className: 'mt-3', name: \"amount\", label: \"Amount\", placeholder: \"0.1\", register: register })),\r\n            React.createElement(\"button\", { className: \"button text-white py-2 rounded-lg text-xl px-14 bg-gradient-to-br from-emerald-500 via-emerald-500 to-emerald-700 w-full mt-6\", type: \"submit\" },\r\n                \"Transfer \",\r\n                React.createElement(\"span\", { className: 'symbol' }, symbol)))));\r\n};\r\nexport default ERC20TransferFrom;\r\n//# sourceMappingURL=ERC20TransferFrom.js.map","references":["/Users/kames/workspace/kames/code/turbo-eth/node_modules/.pnpm/ethers@5.6.9/node_modules/ethers/lib/index.d.ts","/Users/kames/workspace/kames/code/turbo-eth/node_modules/.pnpm/@types+react@18.0.14/node_modules/@types/react/index.d.ts","/Users/kames/workspace/kames/code/turbo-eth/node_modules/.pnpm/react-hook-form@7.32.2_react@18.2.0/node_modules/react-hook-form/dist/index.d.ts","/Users/kames/workspace/kames/code/turbo-eth/node_modules/.pnpm/classnames@2.3.1/node_modules/classnames/index.d.ts","/Users/kames/workspace/kames/code/turbo-eth/packages/erc20-wagmi/src/useERC20ContractWrite.tsx"],"map":"{\"version\":3,\"file\":\"ERC20TransferFrom.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/ERC20TransferFrom.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,KAAK,EAAE,MAAM,QAAQ,CAAC;AAC/B,OAAO,KAAK,KAAK,MAAM,OAAO,CAAC;AAC/B,OAAO,EAAE,OAAO,EAAE,MAAM,iBAAiB,CAAC;AAC1C,OAAO,UAAU,MAAM,YAAY,CAAC;AACpC,OAAO,qBAAqB,MAAM,yBAAyB,CAAC;AAU5D,MAAM,CAAC,MAAM,iBAAiB,GAAG,CAAC,EAChC,SAAS,EACT,QAAQ,EACR,KAAK,EACL,MAAM,EACiB,EAAE,EAAE;IAC3B,MAAM,SAAS,GAAG,UAAU,CAAC,SAAS,EAAE,mBAAmB,CAAC,CAAC;IAE7D,MAAM,EACF,QAAQ,EACR,YAAY,EACZ,KAAK,EACL,SAAS,EAAE,EAAI,GAChB,GAAG,OAAO,CAAC;QA