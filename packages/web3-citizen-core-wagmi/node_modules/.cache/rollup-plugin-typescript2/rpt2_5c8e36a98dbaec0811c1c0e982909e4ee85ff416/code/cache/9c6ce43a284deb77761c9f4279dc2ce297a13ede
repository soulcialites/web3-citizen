{"code":"import * as React from 'react';\r\nimport { useForm } from 'react-hook-form';\r\nimport classNames from 'classnames';\r\nimport { useNationWrite } from '../hooks/useNationWrite';\r\nimport { useLogError, useLogTransactionWrite } from '../hooks';\r\nimport { utils } from 'ethers';\r\nexport const NationFormRoleRevoke = ({ className, label, onUpdate, contractAddress, }) => {\r\n    const classes_ = classNames(className, 'NationFormRoleRevoke');\r\n    const { register, handleSubmit, watch, formState: {}, } = useForm({\r\n        defaultValues: {\r\n            citizen: '',\r\n            role: '',\r\n        },\r\n    });\r\n    const watchAllFields = watch();\r\n    const { write, error, data } = useNationWrite(contractAddress, 'revokeRole', [\r\n        utils.keccak256(utils.toUtf8Bytes(watchAllFields.role)),\r\n        watchAllFields.citizen,\r\n    ]);\r\n    useLogError(error);\r\n    useLogTransactionWrite(data);\r\n    const onSubmit = (_data) => {\r\n        write();\r\n        if (onUpdate)\r\n            onUpdate(_data);\r\n    };\r\n    return (React.createElement(\"div\", { className: classes_ },\r\n        React.createElement(\"form\", { onSubmit: handleSubmit(onSubmit) },\r\n            React.createElement(\"input\", Object.assign({ className: \"input\", placeholder: \"Nation\" }, register('role', { required: true }))),\r\n            React.createElement(\"input\", Object.assign({ className: \"input mt-2\", placeholder: \"web3oftrust.eth\" }, register('citizen', { required: true }))),\r\n            React.createElement(\"button\", { className: \"btn btn-default my-3 w-full\", type: \"submit\" }, label))));\r\n};\r\nNationFormRoleRevoke.defaultProps = {\r\n    label: 'Revoke Role',\r\n};\r\nexport default NationFormRoleRevoke;\r\n//# sourceMappingURL=NationFormRoleRevoke.js.map","references":["/Users/kames/workspace/soulcialites/web3-citizen/node_modules/.pnpm/@types+react@18.0.14/node_modules/@types/react/index.d.ts","/Users/kames/workspace/soul