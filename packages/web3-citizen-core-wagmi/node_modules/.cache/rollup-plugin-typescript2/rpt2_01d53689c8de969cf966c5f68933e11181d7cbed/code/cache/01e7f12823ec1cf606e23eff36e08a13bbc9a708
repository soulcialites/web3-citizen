{"code":"import * as React from 'react';\r\nimport { useForm } from 'react-hook-form';\r\nimport classNames from 'classnames';\r\nimport { useNationWrite } from '../hooks/useNationWrite';\r\nimport { useLogError, useLogTransactionWrite } from '../hooks';\r\nimport { utils } from 'ethers';\r\nexport const NationFormEnableRole = ({ className, label, onUpdate, contractAddress, }) => {\r\n    const classes_ = classNames(className, 'NationFormEnableRole');\r\n    const { register, handleSubmit, watch, formState: {}, } = useForm({\r\n        defaultValues: {\r\n            role: '',\r\n            adminRole: '',\r\n        },\r\n    });\r\n    const watchAllFields = watch();\r\n    const { write, error, data } = useNationWrite(contractAddress, 'enableRole', [\r\n        utils.keccak256(utils.toUtf8Bytes(watchAllFields.role)),\r\n        utils.keccak256(utils.toUtf8Bytes(watchAllFields.adminRole)),\r\n    ]);\r\n    useLogError(error);\r\n    useLogTransactionWrite(data);\r\n    const onSubmit = (_data) => {\r\n        write();\r\n        if (onUpdate)\r\n            onUpdate(_data);\r\n    };\r\n    return (React.createElement(\"div\", { className: classes_ },\r\n        React.createElement(\"form\", { onSubmit: handleSubmit(onSubmit) },\r\n            React.createElement(\"label\", { className: \"label my-2 block\" }, \"Role\"),\r\n            React.createElement(\"input\", Object.assign({ className: \"input\", placeholder: \"TREASURY\" }, register('role', { required: true }))),\r\n            React.createElement(\"label\", { className: \"label my-2 block\" }, \"Admin Role\"),\r\n            React.createElement(\"input\", Object.assign({ className: \"input\", placeholder: \"FOUNDER\" }, register('adminRole', { required: true }))),\r\n            React.createElement(\"button\", { className: \"btn btn-default my-3 w-full\", type: \"submit\" }, label))));\r\n};\r\nNationFormEnableRole.defaultProps = {\r\n    label: 'Enable Role',\r\n};\r\nexport default NationFormEnableRole;\r\n//# sourceMappingURL=NationFormEnableRole.js.map","references":["/Users/kames/workspace/soulcialites/web3-citizen/node_modules/.pnpm/@types+react@18.0.14/node_modules/@types/react/index.d.ts","/Users/kames/workspace/soulcialites/web3-citizen/node_modules/.pnpm/react-hook-form@7.32.2_react@18.2.0/node_modules/react-hook-form/dist/index.d.ts","/Users/kames/workspace/soulcialites/web3-citizen/node_modules/.pnpm/classnames@2.3.1/node_modules/classnames/index.d.ts","/Users/kames/workspace/soulcialites/web3-citizen/packages/web3-citizen-core-wagmi/src/hooks/useNationWrite.tsx","/Users/kames/workspace/soulcialites/web3-citizen/packages/web3-citizen-core-wagmi/src/hooks/index.ts","/Users/kames/workspace/soulcialites/web3-citizen/node_modules/.pnpm/ethers@5.6.9/node_modules/ethers/lib/index.d.ts"],"map":"{\"version\":3,\"file\":\"NationFormEnableRole.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/Nation/NationFormEnableRole.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,KAAK,MAAM,OAAO,CAAC;AAC/B,OAAO,EAAE,OAAO,EAAE,MAAM,iBAAiB,CAAC;AAC1C,OAAO,UAAU,MAAM,YAAY,CAAC;AACpC,OAAO,EAAE,cAAc,EAAE,MAAM,yBAAyB,CAAC;AACzD,OAAO,EAAE,WAAW,EAAE,sBAAsB,EAAE,MAAM,UAAU,CAAC;AAC/D,OAAO,EAAE,KAAK,EAAE,MAAM,QAAQ,CAAC;AAU/B,MAAM,CAAC,MAAM,oBAAoB,GAAG,CAAC,EACnC,SAAS,EACT,KAAK,EACL,QAAQ,EACR,eAAe,GACW,EAAE,EAAE;IAC9B,MAAM,QAAQ,GAAG,UAAU,CAAC,SAAS,EAAE,sBAAsB,CAAC,CAAC;IAE/D,MAAM,EACJ,QAAQ,EACR,YAAY,EACZ,KAAK,EACL,SAAS,EAAE,EAAE,GACd,GAAG,OAAO,CAAC;QACV,aAAa,EAAE;YACb,IAAI,EAAE,EAAE;YACR,SAAS,EAAE,EAAE;SACd;KACF,CAAC,CAAC;IACH,MAAM,cAAc,GAAG,KAAK,EAAE,CAAC;IAC/B,MAAM,EAAE,KAAK,EAAE,KAAK,EAAE,IAAI,EAAE,GAAG,cAAc,CAAC,eAAe,EAAE,YAAY,EAAE;QAC3E,KAAK,CAAC,SAAS,CAAC,KAAK,CAAC,WAAW,CAAC,cAAc,CAAC,IAAI,CAAC,CAAC;QACvD,KAAK,CAAC,SAAS,CAAC,KAAK,CAAC,WAAW,CAAC,cAAc,CAAC,SAAS,CAAC,CAAC;KAC7D,CAAC,CAAC;IACH,WAAW,CAAC,KAAK,CAAC,CAAC;IACnB,sBAAsB,CAAC,IAAI,CAAC,CAAC;IAC7B,MAAM,QAAQ,GAAG,CAAC,KAAU,EAAE,EAAE;QAC9B,KAAK,EAAE,CAAC;QACR,IAAI,QAAQ;YAAE,QAAQ,CAAC,KAAK,CAAC,CAAC;IAChC,CAAC,CAAC;IAEF,